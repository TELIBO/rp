{
  "name": "marketing-search-tool",
  "version": "1.0.0",
  "description": "Smart internal search tool for marketing documents and assets",
  "main": "server/index.js",
  "scripts": {
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "search",
    "documents",
    "indexing",
    "marketing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lunr": "^2.3.9",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.10.0",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "sqlite3": "^5.1.7"
  }
}
