{
  "name": "marketing-search-tool",
  "version": "1.0.0",
  "description": "Smart internal search tool for marketing documents and assets",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "start": "node server/index.js"
  },
  "keywords": [
    "search",
    "documents",
    "indexing",
    "marketing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@qdrant/js-client-rest": "^1.15.1",
    "@xenova/transformers": "^2.17.2",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "lunr": "^2.3.9",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.10.0",
    "openai": "^6.9.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
